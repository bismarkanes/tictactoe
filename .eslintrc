{
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 5,
    "ecmaFeatures": {
      "jsx": false
    }
  },
  "rules": {
    "semi": 2,
    "no-console": 0,
    "no-unused-vars": ["error", { "vars": "all", "args": "none", "ignoreRestSiblings": false }],
    "no-undef": 0
  }
}
